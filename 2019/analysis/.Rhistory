mutate(
telework_cat = case_when(
(as.numeric(upd_telework_time) > 0 & as.numeric(upd_telework_time) <= 60)  ~ "Part-time",
(as.numeric(upd_telework_time) > 60 & as.numeric(upd_telework_time) < 360) ~ "Part-time",
